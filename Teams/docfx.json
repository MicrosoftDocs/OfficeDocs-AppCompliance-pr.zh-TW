{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.svg",
          "**/*.pdf"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "contributors_to_exclude": [
        "dstrome",
        "dstrome2"
      ],
      "hideEdit": true,
      "ocv-translation-feedback": true,
      "breadcrumb_path": "/teams-app-certification/breadcrumb/toc.json",
      "searchScope": [
        "Microsoft Teams"
      ],
      "extendBreadcrumb": true
    },
    "fileMetadata": {},
    "template": [],
    "dest": "OfficeDocs-AppCompliance-Teams",
    "markdownEngineName": "markdig"
  }
}